{
  "title": "Polkadot API Book",
  "gitbook": ">=2.4.0",
  "plugins": [
    "include-codeblock",
    "insert-logo-link",
    "mermaid-gb3",
    "-highlight",
    "sunlight-highlighter"
  ],
  "root": "docs",
  "pluginsConfig": {
    "insert-logo-link": {
      "src": "/logo.png",
      "url": "https://polkadot.js.org"
    },
    "sunlight-highlighter": {
      "theme": "gitbook"
    }
  },
  "styles": {
    "website": "styles/website.css"
  }
}
