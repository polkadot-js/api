      {{#unless @root.isStrict}}
      [index: string]: AugmentedConst<object & Codec>;
      {{/unless}}
      {{~#each items}}
      {{> docs}}
      {{{name}}}: AugmentedConst<{{{type}}}>;
      {{/each}}
    };
    {{/each}}
  }
}
