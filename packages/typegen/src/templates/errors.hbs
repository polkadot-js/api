declare module '@polkadot/api-base/types/errors' {
  export interface AugmentedErrors<ApiType extends ApiTypes> {
    {{#each modules}}
    {{{name}}}: {
      {{#each items}}
      {{> docs}}
      {{{name}}}: AugmentedError<ApiType>;
      {{/each}}
      {{#unless @root.isStrict}}
      /**
       * Generic error
       **/
      [key: string]: AugmentedError<ApiType>;
      {{/unless}}
    };
    {{/each}}
  } // AugmentedErrors
} // declare module
